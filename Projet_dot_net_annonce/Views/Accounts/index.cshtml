@model Projet_dot_net_annonce.ViewModels.RgisterListViewMode
@{
    Layout = "_Layout - Copier.cshtml";


}

<table class="table table-striped">
    <thead>
        <tr>

            <th scope="col">Nom</th>
            <th scope="col">Email</th>
            <th scope="col">Action</th>
         
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Users)
        {

            
            <tr>
                <td scope="row">@item.UserName</td>
                <td>@item.Email</td>
                <td> <a asp-controller="Accounts" asp-action="Delet" asp-route-id="@item.Id" type="button" class="btn btn-sm btn-outline-secondary">Supprimer</a>

            </tr>
        }
    </tbody>
</table>
